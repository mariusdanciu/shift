<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
<filter>
  <filter-name>ShiftFilter</filter-name>
  <display-name>Shift Filter</display-name>
  <description>The Filter that intercepts shift calls</description>
  <filter-class>net.shift.http.jee.ShiftFilter</filter-class>
</filter>
  

<filter-mapping>
  <filter-name>ShiftFilter</filter-name>
  <url-pattern>/*</url-pattern>
</filter-mapping>

</web-app>

